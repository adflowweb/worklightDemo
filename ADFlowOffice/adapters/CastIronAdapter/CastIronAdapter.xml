<wl:adapter name="CastIronAdapter"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:wl="http://www.worklight.com/integration"
	xmlns:http="http://www.worklight.com/integration/http"
	xmlns:ci="http://www.worklight.com/integration/ci">

	<displayName>CastIronAdapter</displayName>
	<description>CastIronAdapter</description>
	<connectivity>
		<connectionPolicy xsi:type="http:HTTPConnectionPolicyType">
			<protocol>http</protocol>
			<domain>adflow-svr.iptime.org</domain>
			<port>9081</port>			
		</connectionPolicy>
		<loadConstraints maxConcurrentConnectionsPerNode="2" />
	</connectivity>

	<procedure name="startOrchestration"/>
	<procedure name="startOrchestrationFiltered"/>
		<!-- procedure name="startOrchestration_post" securityTest="WLADFlowOfficeSecurityTest"/-->
		<procedure securityTest="WLADFlowOfficeSecurityTest" name="getDummy" />
	<procedure name="startOrchestration_post" />
	<procedure securityTest="WLADFlowOfficeSecurityTest" name="getDummy" /> 
		
</wl:adapter>
